CREATE TABLE stream_data (
 stream_id int,
 user_id int,
 user_name text,
 game_id int,
 stream_type text,
 title text,
 viewer_count int,
 started_at timestamp,
 language text);

 CREATE TABLE game_data (
 game_id int,
 game_name text);

CREATE TABLE game_genres (
game_name text,
game_genres text);
